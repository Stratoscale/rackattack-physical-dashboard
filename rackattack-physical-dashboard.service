[Unit]
Description=Rackattack Dashboard
After=network.target

[Service]
Type=simple
Environment='PYTHONPATH=/usr/share/rackattack-physical-dashboard/rackattack-physical-dashboard.egg'
ExecStart=/bin/sh -c "exec /usr/bin/python -m rackattack.dashboard.main --realtimewebuiRoot=/usr/share/rackattack-physical-dashboard/realtimewebui --dashboardRoot=/usr/share/rackattack-physical-dashboard --rackattackInstances='MyMachine:127.0.0.1,AnotherMachine:127.0.0.2'"

[Install]
WantedBy=multi-user.target
